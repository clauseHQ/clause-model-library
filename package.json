{
  "name": "@clausehq/clause-model-library",
  "version": "0.4.13",
  "description": "A library of reuseable models for use in smart contracts that conform to the Cicero Template Specification",
  "scripts": {
    "licensecheck": "license-check",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "postlint": "npm run licensecheck",
    "test": "mocha",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clauseHQ/clause-model-library.git"
  },
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/clauseHQ/clause-model-library/issues"
  },
  "homepage": "https://github.com/clauseHQ/clause-model-library#readme",
  "license-check-config": {
    "src": [
      "common/**/*",
      "physical/**/*",
      "logical/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "composer-common": "^0.19.8",
    "eslint": "^3.19.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.3.0",
    "license-check": "^1.1.5",
    "mocha": "^5.0.0",
    "chai-as-promised": "^7.1.1"
  }
}
