    "url": "git+https://github.com/clauseHQ/clause-model-library.git"
  },
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/clauseHQ/clause-model-library/issues"
  },
  "homepage": "https://github.com/clauseHQ/clause-model-library#readme",
  "license-check-config": {
    "src": [
