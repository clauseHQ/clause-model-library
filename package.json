{
  "name": "@clausehq/clause-model-library",
  "version": "2.0.0",
  "description": "A library of reuseable Concerto models",
  "license": "UNLICENSED",
  "author": "Clause Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clauseHQ/clause-model-library.git"
  },
  "scripts": {
